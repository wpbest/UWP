<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>Win32 Application for OpenGL ES</Name>
    <Description>A project for creating a Win32 Application for OpenGL ES.</Description>
    <Icon>__TemplateIcon.ico</Icon>
    <ProjectType>VC</ProjectType>
    <DefaultName>ApplicationForOpenGLES</DefaultName>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="hellotriangle.vcxproj" File="hellotriangle.vcxproj" ReplaceParameters="true">
      <ProjectItem ReplaceParameters="true" TargetFileName="$projectname$.vcxproj.filters">hellotriangle.vcxproj.filters</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="HelloTriangleRenderer.cpp">HelloTriangleRenderer.cpp</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="application.cpp">application.cpp</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="pch.cpp">pch.cpp</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="HelloTriangleRenderer.h">HelloTriangleRenderer.h</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="application.h">application.h</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="pch.h">pch.h</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>